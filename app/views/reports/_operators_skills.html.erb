<% @result.keys.each do |operator| %>
  <% unless @result[operator].keys.empty? %>
  <h1><%= operator %></h1>
  <table class="table table-bordered table-hover">
    <thead class="thead-light">
      <tr>
        <% @result[operator].keys.each do |ingroup| %>
          <th colspan="2"><%= ingroup %></th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <tr>
        <% @result[operator].keys.each do |ingroup| %>
          <td>RANK:<br><%= @result[operator][ingroup][:rank] %></td>
          <td>GRADE:<br><%= @result[operator][ingroup][:grade] %></td>
        <% end %>
      </tr>
    </tbody>
  </table>
  <% end %>
<% end %>